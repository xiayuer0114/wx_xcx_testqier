	<package name="/v2/ShengmutitemaiConfig" extends="struts-default" namespace="/v2/ShengmutitemaiConfig">
		<global-allowed-methods>regex:([A-Za-z0-9_$]*)</global-allowed-methods>
		<action name="*" class="com.lymava.qier.activities.action.ShengmutitemaiConfigAction"
				method="{1}">
			<result>${successResultValue}</result>
		</action>
	</package>


</struts>