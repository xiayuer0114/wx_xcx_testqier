				method="{1}">
			<result>${successResultValue}</result>
		</action>
	</package>


	<package name="/v2/ActivitieMerchantRedEnvelope" extends="struts-default" namespace="/v2/ActivitieMerchantRedEnvelope">
		<global-allowed-methods>regex:([A-Za-z0-9_$]*)</global-allowed-methods>
