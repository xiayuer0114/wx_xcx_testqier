.not_record {
  display: flex;
  width: 100%;
  height: 100%;
  justify-content: center;
  align-items: center;
  vertical-align: top;
}

.not_record text{
  line-height: 56rpx;
  color: #6E6D6D;
}




.first_used{
  width: 100%;
  height: 100%;
  position: fixed;
  left: 0;
  top: 0;
  z-index: 20;
  background: rgba(0, 0, 0, 0.57);
  /*display: none;*/
}
.first_used .first_home{
  width: 100%;
  height: 100%;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  text-align: center;
}
.first_used .first_home .info {
  width: 80%;
}
.first_used .first_home .btn_next {
  width: 200rpx;
  height: 69rpx;
}
.first_used .first_detail {
  position: absolute;
  top: 510rpx;
  right: 0;
}
.first_used .first_detail .btn_tel {
  position: absolute;
  top: 0rpx;
  right: 100rpx;
}
.first_used .first_detail .arrow_tel {
  position: absolute;
  top: 60rpx;
  right: 90rpx;
}
.first_used .first_detail .btn_map {
  position: absolute;
  top: 90rpx;
  right: 100rpx;
}
.first_used .first_detail .arrow_map {
  position: absolute;
  top: 150rpx;
  right: 100rpx;
}
.first_used .first_detail .btn_map,
.first_used .first_detail .btn_tel {
  width: 150rpx;
  height: 58rpx;
}
.first_used .first_detail .arrow_tel,
.first_used .first_detail .arrow_map {
  width: 43rpx;
  height: 34rpx;
}
.first_detail {
  z-index: auto !important;
}
.first_detail .shop_phone .btn_icon,
.first_detail .shop_address .btn_icon {
  z-index: 30;
  padding: 6rpx;
  background: white;
  border-radius: 50%;
  right: -8rpx !important;
}
.first_account .account_balance {
  position: relative;
}
.first_account .account_balance .balance{
  z-index: 30;
  padding: 10rpx 20rpx;
  background: white;
  border-radius: 50%;
}

/** 修改默认的navigator点击态 **/
.navigator-hover {
    color:blue;
}
/** 自定义其他点击态样式类 **/
.other-navigator-hover {
    color:red;
}