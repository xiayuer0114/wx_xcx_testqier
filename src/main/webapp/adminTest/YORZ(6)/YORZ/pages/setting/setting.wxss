/* pages/setting/setting.wxss */
.mask{
  width: 100%;
  height: 100%;
  position: fixed;
  top:0;
  z-index: 1;
  background-color: rgba(0, 0, 0, 0.3);
}
.set_view_box{
  width: 100%;
  height: 91.9%;
  position: fixed;
  bottom:0;
  background-color: #fff;
  z-index: 1111;
}
.set_title{
  width: 100%;
  height: 75rpx;
  background-color: #ffd000;
  display: flex;
}
.reset_btn{
  width: 50%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #fff;
}
.finish_btn{
  width: 50%;
  height: 100%;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 30rpx;
  color: #fff;
}
.select_type_view{
  width: 100%;
  height: 250rpx;
  border-bottom: 1rpx solid #ccc;
}
.select_item_view{
  width: 90%;
  height: 80rpx;
  margin:0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  font-size: 30rpx;
}
.underline{
  margin-top:20rpx;
  border-bottom: 1rpx solid #ccc;
  padding-bottom: 20rpx;
}
.tabs{
  width: 95%;
  margin: 0 auto;
  margin-top:20rpx;
  font-size: 26rpx;
  color: #868686;
}
.tabs_view{
  width: 90%;
  height: auto;
  margin:0 auto;
  margin-top:15rpx;
  display: flex;
  flex-wrap: wrap;
  
}
.tabs_item{
  width: 23%;
  height: 50rpx;
  border-radius: 10rpx;
  margin:10rpx 5rpx;
  border:1rpx solid #ccc;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  color: #333;
}

.mo_view{
  width: 88%;
  height: 80rpx;
  margin: 0 auto;
  margin-top:65rpx;
  position: relative;
  display: flex;
  
  
}
.slider_line{
  width: 100%;
  height: 10rpx;
  background: #ffd000;
  position: relative;
}
/* .slider_line2{
  width: 100%;
  height: 10rpx;
  background: #ffd000;
  position: relative;
} */
.circle_point{
  width: 50rpx;
  height: 50rpx;
  border: 1rpx solid #ffd000;
  border-radius: 50%;
  z-index: 11;
  background-color: #fff;
  position: absolute;
  top:-20rpx;
}
.circle_point2{
  width: 50rpx;
  height: 50rpx;
  border: 1rpx solid #ffd000;
  border-radius: 50%;
  z-index: 11;
  background-color: #fff;
  position: absolute;
  top:-20rpx;
  left: 100%;
}
.min_fee{
  font-size: 30rpx;
  color: #999;
  position: absolute;
  left: 0;
  top:30rpx;
}
.max_fee{
  font-size: 30rpx;
  color: #999;
  position: absolute;
  right: 0rpx;
  top:30rpx;
}